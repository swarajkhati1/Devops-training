pipeline {
  agent any
  
  stages {
    
    stage("build") {
      
      steps{
        echo 'Here we are bulding the app!'
        
      }
    }
    
    stage("test") {
      
      steps{
        echo 'Here we are testing the app!'
        
      }
    }
    
    stage("deploy") {
      
      steps{
        echo 'Here we are doing the deployment of the app!'
        
      }
    }
  }
}
